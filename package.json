{
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "config": "node ./cypress/main/utils/data/JSONParser.js",
    "config:parallel": "node ./cypress/main/utils/data/JSONParser.js --parallel",
    "load:parallel": "npm run config:parallel && cypress run --headed --browser chrome",
    "test": "npm run config && cypress run --headed --browser chrome",
    "test:split": "npm run config && cypress run --browser chrome --env split=true",
    "test:parallel": "cypress-parallel -s load:parallel -w './cypress/artifacts/parallel-weights.json' -t 3 -d './cypress/test/specs/kaspiPay*.js' -b",
    "start:ci": "npm run build && npm run start",
    "test:ci": "WAIT_ON_TIMEOUT=900000 start-server-and-test start:ci http://localhost:3001 test",
    "test:ci:parallel": "WAIT_ON_TIMEOUT=900000 start-server-and-test start:ci http://localhost:3001 test:parallel",
    "test:ci:split": "WAIT_ON_TIMEOUT=900000 start-server-and-test start:ci http://localhost:3001 test:split"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.40.0",
    "@testing-library/cypress": "^9.0.0",
    "allure-commandline": "^2.23.1",
    "axios": "^0.21.1",
    "cypress": "12.14.0",
    "cypress-localstorage-commands": "^2.2.4",
    "cypress-log-filter": "^1.0.4",
    "cypress-parallel": "^0.14.0",
    "cypress-plugin-multiple-click": "^1.0.0",
    "cypress-split": "^1.18.5",
    "cypress-xpath": "^2.0.1",
    "dotenv": "^16.3.1",
    "moment": "^2.29.4",
    "mysql2": "^3.4.1",
    "qs": "^6.11.2",
    "start-server-and-test": "^2.0.0",
    "xml2js": "^0.6.2"
  }
}